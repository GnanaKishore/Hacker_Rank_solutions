#include <iostream>
#include <cstdio>
using namespace std;

/*
Using the max and min functions in c++ we can compare two inputs and get the required output in an easy manner.
*/

int main() {
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    int e,f;
    e =max(a,b);
    f=max(c,d);
    int g=max(e,f);
    
    cout<<g<<endl;
    
    return 0;
}
